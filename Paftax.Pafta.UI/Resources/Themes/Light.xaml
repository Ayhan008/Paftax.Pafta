<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Generic Colors-->
    <SolidColorBrush x:Key="PrimaryBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="PrimaryForeground" Color="#565656"/>
    <SolidColorBrush x:Key="PrimaryBorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="PrimaryHighlightBackground" Color="#f0f0f0"/>
    <SolidColorBrush x:Key="PrimaryHighlightBorderBrush" Color="#f0f0f0"/>
    <!--#endregion-->

    <!--#region Window Colors-->
    <!--Generic Window-->
    <SolidColorBrush x:Key="WindowBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="WindowForeground" Color="#565656"/>
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#bdbdbd"/>
    <!--#endregion-->
    
    <!--#region TitleBar Colors-->
    <!--Generic TitleBar-->
    <SolidColorBrush x:Key="TitleBarBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="TitleBarForeground" Color="#565656"/>
    <SolidColorBrush x:Key="TitleBarBorderBrush" Color="#bdbdbd"/>
    <!--#endregion-->
    
    <!--#region DataGrid Colors-->
    <!--Generic DataGrid-->
    <SolidColorBrush x:Key="DataGridBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="DataGridForeground" Color="#565656"/>
    <SolidColorBrush x:Key="DataGridBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="DataGridColumnHeaderBackground" Color="#dfdfdf"/>
    <SolidColorBrush x:Key="DataGridColumnHeaderForeground" Color="#565656"/>
    <SolidColorBrush x:Key="DataGridColumnHeaderBorderBrush" Color="#d0d0d0"/>

    <SolidColorBrush x:Key="DataGridRowBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="DataGridRowForeground" Color="#565656"/>
    <SolidColorBrush x:Key="DataGridRowBorderBrush" Color="Transparent"/>
    
    <SolidColorBrush x:Key="DataGridRowHoverBackground" Color="#dfdfdf"/>
    <SolidColorBrush x:Key="DataGridRowHoverForeground" Color="#565656"/>
    <SolidColorBrush x:Key="DataGridRowHoverBorderBrush" Color="#d5d5d5"/>
    
    <SolidColorBrush x:Key="DataGridRowSelectedBackground" Color="#a4daf1"/>
    <SolidColorBrush x:Key="DataGridRowSelectedForeground" Color="#565656"/>
    <SolidColorBrush x:Key="DataGridRowSelectedBorderBrush" Color="#68c0e7"/>
    
    <SolidColorBrush x:Key="DataGridGridLinesBrush" Color="#d0d0d0"/>
    <!--#endregion-->
    
    <!--#region Button Colors-->
    <!--Generic Button-->
    <SolidColorBrush x:Key="ButtonBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="#565656"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#bdbdbd"/>
    
    <SolidColorBrush x:Key="ButtonHoverBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="ButtonHoverForeground" Color="#565656"/>
    
    <SolidColorBrush x:Key="ButtonHoverHighlightBackground" Color="#f0f0f0"/>
    <SolidColorBrush x:Key="ButtonHoverHighlightBorderBrush" Color="#f0f0f0"/>
    
    <SolidColorBrush x:Key="ButtonPressedBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="ButtonPressedForeground" Color="#565656"/>
    <SolidColorBrush x:Key="ButtonPressedBorderBrush" Color="#bdbdbd"/>
    
    <SolidColorBrush x:Key="ButtonPressedHighlightBackground" Color="#f0f0f0"/>
    <SolidColorBrush x:Key="ButtonPressedHighlightBorderBrush" Color="#f0f0f0"/>

    <!-- Result Button-->
    <SolidColorBrush x:Key="ResultButtonBackground" Color="#0696d7"/>
    <SolidColorBrush x:Key="ResultButtonForeground" Color="#f5f5f5"/>
    <SolidColorBrush x:Key="ResultButtonBorderBrush" Color="#58b8e4"/>

    <SolidColorBrush x:Key="ResultButtonDisabledBackground" Color="#9bd5ef"/>
    <SolidColorBrush x:Key="ResultButtonDisabledForeground" Color="#fefefe"/>
    <SolidColorBrush x:Key="ResultButtonDisabledBorderBrush" Color="#b9e2f3"/>

    <!--TitleBar Close Button-->
    <SolidColorBrush x:Key="TitleBarCloseButtonBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="TitleBarCloseButtonHoverBackground" Color="#C50F1F"/>
    <SolidColorBrush x:Key="TitleBarCloseButtonPressedBackground" Color="#C50F1F"/>
    
    <!--TitleBar Generic Button-->
    <SolidColorBrush x:Key="TitleBarButtonBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="TitleBarButtonHoverBackground" Color="#f2f2f2"/>
    <SolidColorBrush x:Key="TitleBarButtonPressedBackground" Color="#e6e6e6"/>
    <!--#endregion-->

    <!--#region  TextBox Colors-->
    <!--Generic TextBox-->
    <SolidColorBrush x:Key="TextBoxBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="TextBoxForeground" Color="#565656"/>
    <SolidColorBrush x:Key="TextBoxBorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="TextBoxClickedBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="TextBoxClickedForeground" Color="#565656"/>
    <SolidColorBrush x:Key="TextBoxClickedBorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="TextBoxHoverHighlightBackground" Color="#ececec"/>
    <SolidColorBrush x:Key="TextBoxHoverHighlightBorderBrush" Color="#ababab"/>

    <SolidColorBrush x:Key="TextBoxClickedHighlightBackground" Color="#a8daf1"/>
    <SolidColorBrush x:Key="TextBoxClickedHighlightBorderBrush" Color="#4eb4e3"/>
    <!--#endregion-->

    <!--#region  SearchBox Colors-->
    <!--Generic SearchBox-->
    <SolidColorBrush x:Key="SearchBoxBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="SearchBoxForeground" Color="#565656"/>
    <SolidColorBrush x:Key="SearchBoxBorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="SearchBoxClickedBackgroundBrush" Color="#ffffff"/>
    <SolidColorBrush x:Key="SearchBoxClickedForeground" Color="#565656"/>
    <SolidColorBrush x:Key="SearchBoxClickedBorderBrush" Color="#bdbdbd"/>
    
    <SolidColorBrush x:Key="SearchBoxHoverHighlightBackground" Color="#ececec"/>
    <SolidColorBrush x:Key="SearchBoxHoverHighlightBorderBrush" Color="#ababab"/>

    <SolidColorBrush x:Key="SearchBoxClickedHighlightBackground" Color="#a8daf1"/>
    <SolidColorBrush x:Key="SearchBoxClickedHighlightBorderBrush" Color="#4eb4e3"/>
    
    <SolidColorBrush x:Key="SearchBoxIconBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="SearchBoxIconForeground" Color="#565656"/>
    <SolidColorBrush x:Key="SearchBoxIconBorderBrush" Color="Transparent"/>

    <SolidColorBrush x:Key="SearchBoxPlaceholderBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="SearchBoxPlaceholderForeground" Color="#bdbdbd"/>
    <SolidColorBrush x:Key="SearchBoxPlaceholderBorderBrush" Color="Transparent"/>
    <!--#endregion-->
    
    <!--#region  RadioButton Colors-->
    <!--Generic RadioButton-->
    <SolidColorBrush x:Key="RadioButtonBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="RadioButtonForeground" Color="#565656"/>
    <SolidColorBrush x:Key="RadioButtonBorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="RadioButtonCheckedBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="RadioButtonCheckedForeground" Color="#565656"/>
    <SolidColorBrush x:Key="RadioButtonCheckedBorderBrush" Color="#bdbdbd"/>
    <!--#endregion-->

    <!--#region  ScrollBar Colors-->
    <!--Generic ScrollBar-->
    <SolidColorBrush x:Key="ScrollBarBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="ScrollBarForeground" Color="#e6e6e6"/>
    <SolidColorBrush x:Key="ScrollBarBorderBrush" Color="#ffffff"/>

    <SolidColorBrush x:Key="ScrollBarThumbBackground" Color="#e6e6e6"/>
    <SolidColorBrush x:Key="ScrollBarThumbBorderBrush" Color="#e6e6e6"/>
    
    <SolidColorBrush x:Key="ScrollBarThumbHoverBackground" Color="#bfbfbf"/>
    <SolidColorBrush x:Key="ScrollBarThumbHoverBorderBrush" Color="#bfbfbf"/>
    <!--#endregion-->
    
    <!--#region  ComboBox Colors-->
    <!-- ComboBox -->
    <SolidColorBrush x:Key="ComboBoxBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="ComboBoxBorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="ComboBoxHoverHighlightBackground" Color="#576276"/>
    <SolidColorBrush x:Key="ComboBoxHoverHighlightBorderBrush" Color="#b9d0eb"/>
    <!--#endregion-->

    <!--#region  CheckBox Colors-->
    <!--Generic CheckBox-->
    <SolidColorBrush x:Key="CheckBoxBackground" Color="#ffffff"/>
    <SolidColorBrush x:Key="CheckBoxForeground" Color="Transparent"/>
    <SolidColorBrush x:Key="CheckBoxBorderBrush" Color="#bdbdbd"/>

    <SolidColorBrush x:Key="CheckBoxCheckedBackground" Color="#565656"/>
    <SolidColorBrush x:Key="CheckBoxCheckedForeground" Color="#ffffff"/>
    <SolidColorBrush x:Key="CheckBoxCheckedBorderBrush" Color="#565656"/>
    
    <SolidColorBrush x:Key="CheckBoxHoverHighlightBackground" Color="#efefef"/>
    <SolidColorBrush x:Key="CheckBoxHoverHighlightBordeBrush" Color="#efefef"/>
    
    <SolidColorBrush x:Key="CheckBoxPressedHighlightBackground" Color="#d3d3d3"/>
    <SolidColorBrush x:Key="CheckBoxPressedHighlightBorderBrush" Color="#d3d3d3"/>
    <!--#endregion-->
    
    <!--#region  Border Colors-->
    <!--Border-->
    <SolidColorBrush x:Key="BorderBackground" Color="#bdbdbd"/>
    <SolidColorBrush x:Key="BorderBorderBrush" Color="#bdbdbd"/>
    <!--#endregion-->

    <!--#region  FontIcon Colors-->
    <SolidColorBrush x:Key="FontIconForeground" Color="#565656"/>
    <!--#endregion-->
</ResourceDictionary>